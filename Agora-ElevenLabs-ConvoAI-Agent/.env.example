# Agora Settings
AGORA_APP_ID=your_agora_app_id
AGORA_APP_CERT=your_agora_cert

# ElevenLabs Settings
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_AGENT_ID=your_agent_id
ELEVENLABS_MODEL=optional_model_name  # Optional: The model to use for the conversation
ELEVENLABS_VOICE_ID=voice_id  # The ID of the voice to use
ELEVENLABS_TEMPERATURE=0.8  # Optional: Controls response randomness (0.0-1.0)

# Optional: Dynamic variables for the conversation (as JSON string)
ELEVENLABS_DYNAMIC_VARS={"key": "value"}

# Optional: Extra configuration for the conversation (as JSON string)
ELEVENLABS_EXTRA_CONFIG={"setting": "value"}

# Server Settings
SERVER_PORT=8080

# Debug Settings
WRITE_RTC_PCM=false
LOG_LEVEL=INFO 